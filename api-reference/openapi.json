{
  "openapi": "3.1.0",
  "info": {
    "title": "FastAPI",
    "version": "0.1.0"
  },
  "paths": {
    "/v0/search": {
      "post": {
        "tags": [
          "channel3-api"
        ],
        "summary": "Search",
        "operationId": "search_v0_search_post",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/SearchRequest"
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {
                  "items": {
                    "$ref": "#/components/schemas/Product"
                  },
                  "type": "array",
                  "title": "Response Search V0 Search Post"
                }
              }
            }
          },
          "401": {
            "description": "Unauthorized - Invalid or missing authentication",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorResponse"
                },
                "example": {
                  "detail": "Invalid or missing authentication token"
                }
              }
            }
          },
          "422": {
            "description": "Validation Error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ValidationError"
                },
                "example": {
                  "detail": [
                    {
                      "loc": [
                        "body",
                        "query"
                      ],
                      "msg": "field required",
                      "type": "value_error.missing"
                    }
                  ]
                }
              }
            }
          },
          "500": {
            "description": "Internal Server Error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorResponse"
                },
                "example": {
                  "detail": "Internal server error occurred"
                }
              }
            }
          }
        }
      }
    },
    "/v0/products/{product_id}": {
      "get": {
        "tags": [
          "channel3-api"
        ],
        "summary": "Get Product Detail",
        "description": "Get detailed information about a specific product by its ID.\n\nArgs:\n    product_id: The unique identifier of the product\n\nReturns:\n    ProductGraphProduct: The complete product information\n\nRaises:\n    HTTPException: 404 if product not found",
        "operationId": "get_product_detail_v0_products__product_id__get",
        "parameters": [
          {
            "name": "product_id",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string",
              "title": "Product Id"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ProductDetail"
                }
              }
            }
          },
          "401": {
            "description": "Unauthorized - Invalid or missing authentication",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorResponse"
                },
                "example": {
                  "detail": "Invalid or missing authentication token"
                }
              }
            }
          },
          "404": {
            "description": "Product not found",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorResponse"
                },
                "example": {
                  "detail": "Product with ID 'xyz123' not found"
                }
              }
            }
          },
          "422": {
            "description": "Validation Error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ValidationError"
                },
                "example": {
                  "detail": [
                    {
                      "loc": [
                        "path",
                        "product_id"
                      ],
                      "msg": "string does not match regex",
                      "type": "value_error.str.regex"
                    }
                  ]
                }
              }
            }
          },
          "500": {
            "description": "Internal Server Error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorResponse"
                },
                "example": {
                  "detail": "Internal server error occurred"
                }
              }
            }
          }
        }
      }
    },
    "/": {
      "get": {
        "summary": "Root",
        "operationId": "root__get",
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {}
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "AvailabilityStatus": {
        "type": "string",
        "enum": [
          "InStock",
          "OutOfStock",
          "PreOrder",
          "LimitedAvailability",
          "BackOrder",
          "Discontinued",
          "SoldOut",
          "Unknown"
        ],
        "title": "AvailabilityStatus"
      },
      "FamilyMember": {
        "properties": {
          "id": {
            "type": "string",
            "title": "Id"
          },
          "title": {
            "type": "string",
            "title": "Title"
          },
          "image_url": {
            "type": "string",
            "title": "Image Url"
          }
        },
        "type": "object",
        "required": [
          "id",
          "title",
          "image_url"
        ],
        "title": "FamilyMember"
      },
      "MerchantOffering": {
        "properties": {
          "url": {
            "type": "string",
            "title": "Url",
            "default": "https://buy.trychannel3.com"
          },
          "merchant_name": {
            "type": "string",
            "title": "Merchant Name"
          },
          "price": {
            "$ref": "#/components/schemas/Price"
          },
          "availability": {
            "$ref": "#/components/schemas/AvailabilityStatus"
          }
        },
        "type": "object",
        "required": [
          "merchant_name",
          "price",
          "availability"
        ],
        "title": "MerchantOffering",
        "description": "A merchant offering a product\nDiffers from MerchantProductOffering in that it has no commission percentage -- this is external"
      },
      "Price": {
        "properties": {
          "price": {
            "type": "number",
            "title": "Price",
            "description": "The current price of the product, including any discounts."
          },
          "compare_at_price": {
            "anyOf": [
              {
                "type": "number"
              },
              {
                "type": "null"
              }
            ],
            "title": "Compare At Price",
            "description": "The original price of the product before any discounts."
          },
          "currency": {
            "type": "string",
            "title": "Currency",
            "description": "The currency code of the product."
          }
        },
        "type": "object",
        "required": [
          "price",
          "currency"
        ],
        "title": "Price"
      },
      "Product": {
        "properties": {
          "id": {
            "type": "string",
            "title": "Id"
          },
          "score": {
            "type": "number",
            "title": "Score"
          },
          "brand_name": {
            "type": "string",
            "title": "Brand Name"
          },
          "title": {
            "type": "string",
            "title": "Title"
          },
          "description": {
            "type": "string",
            "title": "Description"
          },
          "image_url": {
            "type": "string",
            "title": "Image Url"
          },
          "offers": {
            "items": {
              "$ref": "#/components/schemas/MerchantOffering"
            },
            "type": "array",
            "title": "Offers"
          },
          "family": {
            "items": {
              "$ref": "#/components/schemas/FamilyMember"
            },
            "type": "array",
            "title": "Family",
            "default": []
          }
        },
        "type": "object",
        "required": [
          "id",
          "score",
          "brand_name",
          "title",
          "description",
          "image_url",
          "offers"
        ],
        "title": "Product",
        "description": "A product"
      },
      "ProductDetail": {
        "properties": {
          "brand_id": {
            "type": "string",
            "title": "Brand Id"
          },
          "brand_name": {
            "type": "string",
            "title": "Brand Name"
          },
          "title": {
            "type": "string",
            "title": "Title"
          },
          "description": {
            "type": "string",
            "title": "Description"
          },
          "image_urls": {
            "items": {
              "type": "string"
            },
            "type": "array",
            "title": "Image Urls"
          },
          "merchant_offerings": {
            "items": {
              "$ref": "#/components/schemas/MerchantOffering"
            },
            "type": "array",
            "title": "Merchant Offerings"
          },
          "gender": {
            "type": "string",
            "enum": [
              "na",
              "men",
              "women"
            ],
            "title": "Gender",
            "default": "na"
          },
          "materials": {
            "anyOf": [
              {
                "items": {
                  "type": "string"
                },
                "type": "array"
              },
              {
                "type": "null"
              }
            ],
            "title": "Materials"
          },
          "key_features": {
            "items": {
              "type": "string"
            },
            "type": "array",
            "title": "Key Features",
            "default": []
          },
          "family_members": {
            "items": {
              "$ref": "#/components/schemas/FamilyMember"
            },
            "type": "array",
            "title": "Family Members",
            "default": []
          }
        },
        "type": "object",
        "required": [
          "brand_id",
          "brand_name",
          "title",
          "description",
          "image_urls",
          "merchant_offerings"
        ],
        "title": "ProductDetail",
        "description": "A product with detailed information"
      },
      "SearchFilters": {
        "properties": {
          "colors": {
            "anyOf": [
              {
                "items": {
                  "type": "string"
                },
                "type": "array"
              },
              {
                "type": "null"
              }
            ],
            "title": "Colors"
          },
          "materials": {
            "anyOf": [
              {
                "items": {
                  "type": "string"
                },
                "type": "array"
              },
              {
                "type": "null"
              }
            ],
            "title": "Materials"
          },
          "min_price": {
            "anyOf": [
              {
                "type": "number"
              },
              {
                "type": "null"
              }
            ],
            "title": "Min Price"
          },
          "max_price": {
            "anyOf": [
              {
                "type": "number"
              },
              {
                "type": "null"
              }
            ],
            "title": "Max Price"
          }
        },
        "type": "object",
        "title": "SearchFilters"
      },
      "SearchRequest": {
        "properties": {
          "query": {
            "anyOf": [
              {
                "type": "string"
              },
              {
                "type": "null"
              }
            ],
            "title": "Query"
          },
          "image_url": {
            "anyOf": [
              {
                "type": "string"
              },
              {
                "type": "null"
              }
            ],
            "title": "Image Url"
          },
          "base64_image": {
            "anyOf": [
              {
                "type": "string"
              },
              {
                "type": "null"
              }
            ],
            "title": "Base64 Image"
          },
          "filters": {
            "anyOf": [
              {
                "$ref": "#/components/schemas/SearchFilters"
              },
              {
                "type": "null"
              }
            ]
          },
          "limit": {
            "anyOf": [
              {
                "type": "integer"
              },
              {
                "type": "null"
              }
            ],
            "title": "Limit",
            "default": 20
          }
        },
        "type": "object",
        "title": "SearchRequest"
      },
      "ErrorResponse": {
        "properties": {
          "detail": {
            "type": "string",
            "title": "Detail",
            "description": "A human-readable explanation of the error"
          }
        },
        "type": "object",
        "required": [
          "detail"
        ],
        "title": "ErrorResponse",
        "description": "Standard error response format"
      },
      "ValidationError": {
        "properties": {
          "detail": {
            "items": {
              "$ref": "#/components/schemas/ValidationErrorDetail"
            },
            "type": "array",
            "title": "Detail"
          }
        },
        "type": "object",
        "required": [
          "detail"
        ],
        "title": "ValidationError",
        "description": "Validation error response format"
      },
      "ValidationErrorDetail": {
        "properties": {
          "loc": {
            "items": {
              "anyOf": [
                {
                  "type": "string"
                },
                {
                  "type": "integer"
                }
              ]
            },
            "type": "array",
            "title": "Location"
          },
          "msg": {
            "type": "string",
            "title": "Message"
          },
          "type": {
            "type": "string",
            "title": "Error Type"
          }
        },
        "type": "object",
        "required": [
          "loc",
          "msg",
          "type"
        ],
        "title": "ValidationErrorDetail"
      }
    }
  }
}