{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "Channel3",
  "colors": {
    "primary": "#5D3FD3",
    "light": "#5D3FD3",
    "dark": "#5D3FD3"
  },
  "favicon": "/favicon.svg",
  "languages": [
    { "label": "TypeScript", "syntax": "typescript" },
    { "label": "Python", "syntax": "python" }
  ],

  "contextual": {
    "options": ["copy", "view", "chatgpt", "claude", "cursor"]
  },
  "navigation": {
    "groups": [
      {
        "group": "First steps",
        "pages": ["index", "quickstart"]
      },
      {
        "group": "API",
        "pages": [
          {
            "group": "Product",
            "pages": [
              "api-reference/endpoint/search",
              "api-reference/endpoint/product-detail",
              "api-reference/endpoint/enrich"
            ]
          },
          {
            "group": "Brand",
            "pages": [
              "api-reference/endpoint/brands",
              "api-reference/endpoint/brand-detail"
            ]
          },
          "categories",
          {
            "group": "Websites",
            "pages": ["api-reference/endpoint/websites"]
          }
        ]
      },
      {
        "group": "SDK",
        "pages": ["sdk"]
      },
      {
        "group": "MCP",
        "pages": ["mcp-overview"]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "Website",
          "icon": "globe",
          "href": "https://trychannel3.com"
        },
        {
          "anchor": "Support",
          "icon": "envelope",
          "href": "mailto:support@trychannel3.com"
        }
      ]
    }
  },
  "logo": {
    "light": "/logo/small-light.svg",
    "dark": "/logo/small-dark.svg"
  },
  "navbar": {
    "links": [
      {
        "label": "Support",
        "href": "mailto:founders@trychannel3.com"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Dashboard",
      "href": "https://trychannel3.com/dashboard"
    }
  }
}
