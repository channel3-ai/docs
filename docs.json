{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "Channel3",
  "api": {
    "examples": {
      "languages": ["python", "javascript", "curl"]
    }
  },
  "colors": {
    "primary": "#5D3FD3",
    "light": "#5D3FD3",
    "dark": "#5D3FD3"
  },
  "favicon": "/favicon.svg",
  "languages": [
    { "label": "JavaScript", "syntax": "javascript" },
    { "label": "Python", "syntax": "python" }
  ],

  "contextual": {
    "options": ["copy", "view", "chatgpt", "claude", "cursor", "mcp", "vscode"]
  },
  "navigation": {
    "groups": [
      {
        "group": "First steps",
        "pages": ["index", "quickstart"]
      },
      {
        "group": "API",
        "openapi": "https://app.stainless.com/api/spec/documented/public-sdk/openapi.documented.yml",
        "pages": [
          {
            "group": "Product",
            "pages": [
              "POST /v0/search",
              "GET /v0/products/{product_id}",
              "POST /v0/enrich"
            ]
          },
          {
            "group": "Brands",
            "pages": ["GET /v0/brands"]
          },
          "categories",
          {
            "group": "Websites",
            "pages": ["GET /v0/websites"]
          },
          {
            "group": "Price Tracking",
            "pages": [
              "POST /v0/price-tracking/start",
              "POST /v0/price-tracking/stop",
              "GET /v0/price-tracking/history/{canonical_product_id}",
              "GET /v0/price-tracking/subscriptions",
              "price-tracking-webhooks"
            ]
          },
          "pricing"
        ]
      },
      {
        "group": "SDK",
        "pages": ["sdk"]
      },
      {
        "group": "MCP",
        "pages": ["mcp-overview"]
      },
      {
        "group": "Demos",
        "pages": ["demos/shopping-assistant", "demos/agentic-storefront"]
      },
      {
        "group": "FAQ",
        "pages": ["faq"]
      }
    ]
  },
  "logo": {
    "light": "/logo/channel3-logo-wide-light.svg",
    "dark": "/logo/channel3-logo-wide-dark.svg"
  },
  "navbar": {
    "links": [
      {
        "label": "Support",
        "href": "mailto:support@trychannel3.com"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Dashboard",
      "href": "https://trychannel3.com/dashboard"
    }
  }
}
